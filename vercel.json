{
  "buildCommand": "vite build",
  "outputDirectory": "dist",
  "framework": "vite",
  "functions": {
    "api/[[...route]].ts": {
      "includeFiles": "api/_**/*"
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/[[...route]]" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
